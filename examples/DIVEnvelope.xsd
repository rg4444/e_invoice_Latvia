      <xs:element name="DocumentKindCode">
        <xs:annotation>
          <xs:documentation xml:lang="lv">Dokumenta veida kods</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="DocumentKindVersion">
        <xs:annotation>
          <xs:documentation xml:lang="lv">Dokumenta tipa versija</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50" />
